{
  "extends": ["next", "next/core-web-vitals", "prettier"],
  "rules": {
    "semi": ["error", "always"],
    "no-unused-vars": [1, { "vars": "all", "args": "after-used" }],
    "import/order": [
      "error",
      {
        "alphabetize": {
          "order": "asc"
        }
      }
    ]
  }
}
